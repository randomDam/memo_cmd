                <ITEXT CH="L'Arborescence des fichiers ext organisée de la facon suivante :"/>
                <para PARENT="ParaTitre"/>
                <ITEXT CPARENT="commande" CH="/"/>
                <ITEXT CH=" : La racine du disque dur"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="/bin"/>
                <ITEXT CH=" : les commandes de base (mv, rm, ls...)"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="/dev"/>
                <ITEXT CH=" : les 'devices', accès aux périphériques du système"/>
                <para PARENT="ParaNormal"/>
                <tab/>
                <ITEXT CPARENT="commande" CH="/dev/stdin"/>
                <ITEXT CH=" : désigne l’entrée standard"/>
                <para PARENT="ParaNormal"/>
                <tab/>
                <ITEXT CPARENT="commande" CH="/dev/stdout"/>
                <ITEXT CH=" : désigne la sortie standard"/>
                <para PARENT="ParaNormal"/>
                <tab/>
                <ITEXT CPARENT="commande" CH="/dev/stderr"/>
                <ITEXT CH=" : désigne la sortie d’erreur standard"/>
                <para PARENT="ParaNormal"/>
                <tab/>
                <tab/>
                <ITEXT CH="( L’entrée (stdin), la sortie (stdout) et l’erreur standard (stderr) ont respectivement les descripteurs de fichiers 0, 1 et 2. )"/>
                <para PARENT="ParaNormal"/>
                <tab/>
                <ITEXT CPARENT="commande" CH="/dev/tty"/>
                <ITEXT CH="  désigne le terminal auquel vous êtes connecté"/>
                <para PARENT="ParaNormal"/>
                <tab/>
                <ITEXT CPARENT="commande" CH="/dev/null"/>
                <ITEXT CH=" : Ce fichier se comporte comme un « trou noir » en absorbant toute redirection sans être modifié."/>
                <para PARENT="ParaNormal"/>
                <tab/>
                <ITEXT CPARENT="commande" CH="/dev/zero"/>
                <ITEXT CH=" : Ce fichier renvoie des octets nuls à l’infini."/>
                <para PARENT="ParaNormal"/>
                <tab/>
                <ITEXT CPARENT="commande" CH="/dev/urandom"/>
                <ITEXT CH=" : Ce fichier renvoie des octets aléatoires à l’infini."/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="/etc"/>
                <ITEXT CH=" : les fichiers de configuration"/>
                <para PARENT="ParaNormal"/>
                <tab CPARENT="commande"/>
                <ITEXT CPARENT="commande" CH="/etc/crontab"/>
                <ITEXT CH=" : configuration de crontab"/>
                <para PARENT="ParaNormal"/>
                <tab CPARENT="commande"/>
                <ITEXT CPARENT="commande" CH="/etc/fstab"/>
                <ITEXT CH=" : configuration des partitions et des points de montage"/>
                <para PARENT="ParaNormal"/>
                <tab CPARENT="commande"/>
                <ITEXT CPARENT="commande" CH="/etc/group"/>
                <ITEXT CH=" : la liste des groupes"/>
                <para PARENT="ParaNormal"/>
                <tab CPARENT="commande"/>
                <ITEXT CPARENT="commande" CH="/etc/hosts"/>
                <ITEXT CH=" : correspondance adresse IP - Nom de machine codé en dur"/>
                <para PARENT="ParaNormal"/>
                <tab CPARENT="commande"/>
                <ITEXT CPARENT="commande" CH="/etc/passwd"/>
                <ITEXT CH=" : La liste des utilisateurs"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="/home"/>
                <ITEXT CH=" : les répertoires des utilisateurs"/>
                <para PARENT="ParaNormal"/>
                <tab/>
                <ITEXT CPARENT="commande" CH="/home/user"/>
                <ITEXT CH=" : Répertoire d'un utilisateur (Vous êtes ici (^-^) )"/>
                <para PARENT="ParaNormal"/>
                <tab/>
                <ITEXT CPARENT="commande" CH="/home/user/bin"/>
                <ITEXT CH=" : Répertoire des exécutable de l'utilisateur (ce qui les rends accessible depuis le terminal)"/>
                <para PARENT="ParaNormal"/>
                <tab/>
