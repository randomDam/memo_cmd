            <StoryText>
                <DefaultStyle PARENT="ParaNormal"/>
                <ITEXT CH="dpkg/apt : Installation de package"/>
                <para PARENT="ParaTitre"/>
                <ITEXT CH="Tout d'abord, "/>
                <ITEXT CPARENT="commande" CH="apt-get"/>
                <ITEXT CH=" est une sur-couche de dpkg. dpkg se charge de toute la gestion de l'installation/désinstallation des packages .deb (les packages). "/>
                <ITEXT CPARENT="commande" CH="apt-get"/>
                <ITEXT CH=" permet lui les téléchargements automatiques, la gestion des dépendances, etc..."/>
                <para PARENT="ParaNormal"/>
                <ITEXT CH="Les installations/désinstallations doivent être réalisées en tant que root, les recherches peuvent être faites par n'importe quel utilisateur."/>
                <para PARENT="ParaNormal"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CH="En tant que root : "/>
                <para PARENT="soutitre"/>
                <ITEXT CH="Mise à jour de la base de donnée des logiciels/packages"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="sudo apt update"/>
                <para PARENT="ParaNormal"/>
                <para PARENT="espace"/>
                <ITEXT CH="Installe le programme "/>
                <ITEXT CPARENT="VariableBlue" CH="programme"/>
                <ITEXT CH=" passé en argument."/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="sudo apt install "/>
                <ITEXT CPARENT="VariableBlue" CH="programme"/>
                <para PARENT="ParaNormal"/>
                <para PARENT="espace"/>
                <ITEXT CH="Supprime les fichiers du package passé en argument. Ne touche pas les fichiers de configuration"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="sudo apt remove "/>
                <ITEXT CPARENT="VariableBlue" CH="package"/>
                <para PARENT="ParaNormal"/>
                <para PARENT="espace"/>
                <ITEXT CH="Supprime les fichiers du package passé en argument ET les fichiers de configuration"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="sudo apt remove --purge "/>
                <ITEXT CPARENT="VariableBlue" CH="package"/>
                <para PARENT="ParaNormal"/>
                <para PARENT="espace"/>
                <ITEXT CH="Met a jour tout le système"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="sudo apt upgrade"/>
                <para PARENT="ParaNormal"/>
                <para PARENT="espace"/>
                <ITEXT CH="Reconfigure le package"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="dpkg-reconfigure "/>
                <ITEXT CPARENT="VariableBlue" CH="package"/>
                <para PARENT="ParaNormal"/>
                <para PARENT="espace"/>
                <ITEXT CH="Donne le nom du package ayant installé le fichier /usr/bin/aplay"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="dpkg -S /usr/bin/aplay"/>
                <para PARENT="ParaNormal"/>
                <para PARENT="espace"/>
                <ITEXT CH="Recompile le cache de apt-show-versions"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="apt-show-versions -i"/>
                <para PARENT="ParaNormal"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CH="N'importe quel utilisateur :"/>
                <para PARENT="soutitre"/>
                <ITEXT CH="Affiche la liste des packages qui sont installés sur l'ordinateur [ii] ou en cours de desinstallation (il reste les fichiers de configuration [rc])"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="dpkg -l"/>
                <para PARENT="ParaNormal"/>
                <para PARENT="espace"/>
                <ITEXT CH="Affiche la liste des fichiers installés par le package "/>
                <ITEXT CPARENT="VariableBlue" CH="package"/>
                <ITEXT CH=" installé sur l'ordinateur"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="dpkg -L "/>
                <ITEXT CPARENT="VariableBlue" CH="package"/>
                <para PARENT="ParaNormal"/>
                <para PARENT="espace"/>
                <ITEXT CH="Affiche la liste des packages comportant le "/>
                <ITEXT CPARENT="VariableBlue" CH="mot-clé"/>
                <ITEXT CH=" dans leur description"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="apt search "/>
                <ITEXT CPARENT="VariableBlue" CH="mot-clé"/>
                <para PARENT="ParaNormal"/>
                <para PARENT="espace"/>
                <ITEXT CH="Affiche les caractéristiques et la description du package"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="apt show "/>
                <ITEXT CPARENT="VariableBlue" CH="package"/>
                <para PARENT="ParaNormal"/>
                <para PARENT="espace"/>
                <ITEXT CH="Affiche les dépendances et les packages utilisants le package passé en paramètre"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="apt showpkg "/>
                <ITEXT CPARENT="VariableBlue" CH="package"/>
                <para PARENT="ParaNormal"/>
                <para PARENT="espace"/>
                <ITEXT CH="Packages disponibles pour mise à jour"/>
                <para PARENT="ParaNormal"/>
                <ITEXT CPARENT="commande" CH="apt-show-versions -u -b"/>
                <para PARENT="ParaNormal"/>
            </StoryText>
